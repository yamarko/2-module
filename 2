#include <stdio.h>

int main()
{
    int  N, M[N], min, max,  imin, imax;
    scanf("%i", &N);
    for (int i = 0; i < N; i++)
    {
    	printf("Vvedit M[%i]: \n", i);
    	scanf("%i", &M[i]);
	}
	min = M[0];
	imin = 0;
	max = M[0];
	imax = 0;
    for (int i = 1; i < N; i++)
    {
    	if (M[i] < min)
    	{
    		min = M[i];
    		imin = i; 
		}
	}
	for (int i = 1; i < N; i++)
	{
		if (M[i] > max)
		{
			max = M[i];
    		imax = i;
		}
	}
	M[imin] = max;
	M[imax] = min;
   for (int i = 0; i < N; i++)
    {
    	printf("M[%i]= %i\n", i, M[i]);
	}
}
